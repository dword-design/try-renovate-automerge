{
  "extends": [
    ":semanticCommits",
    ":semanticPrefixFix"
  ],
  "github-actions": {
    "enabled": false
  },
  "labels": [
    "maintenance"
  ],
  "lockFileMaintenance": {
    "automerge": true,
    "enabled": true,
    "semanticCommitType": "chore"
  },
  "rangeStrategy": "replace",
  "regexManagers": [
    {
      "datasourceTemplate": "npm",
      "fileMatch": [
        "\\.js$"
      ],
      "matchStrings": [
        "gitHubAction`(?<depName>.*?)@(?<currentValue>.*?)`"
      ]
    }
  ],
  "semanticCommitScope": null
}
